{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "EX_UPDATES_NATIVE_DEBUG": "1",
        "EXPO_SKIP_NATIVE_BUILDS": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {},
    "local": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "withoutCredentials": true
      },
      "env": {
        "EX_UPDATES_NATIVE_DEBUG": "1",
        "EXPO_SKIP_NATIVE_BUILDS": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
} 